    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "styled-components": "^6.0.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-react": "^7.22.5",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.3",
